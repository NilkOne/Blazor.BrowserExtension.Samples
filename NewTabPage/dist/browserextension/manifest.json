{
  "manifest_version": 3,
  "name": "New Tab Page",
  "description": "Sample for new tab page.",
  "version": "0.1",
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "framework/*",
        "content/*"
      ],
      "matches": [ "<all_urls>" ]
    }
  ],
  "permissions": [
    "bookmarks"
  ]
}